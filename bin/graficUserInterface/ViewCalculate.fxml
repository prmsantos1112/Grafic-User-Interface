<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>

<AnchorPane prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graficUserInterface.ViewControlerCalculate">
   <children>
      <Label layoutX="26.0" layoutY="31.0" text="Calcule Sum">
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="BLACK" />
                  <Stop color="BLACK" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Label>
      <TextField fx:id="txtNumberOne" layoutX="26.0" layoutY="63.0" promptText="First Number" />
      <TextField fx:id="txtNumberTwo" layoutX="26.0" layoutY="100.0" promptText="Second Number" />
      <Button fx:id="btSum" layoutX="26.0" layoutY="145.0" mnemonicParsing="false" onAction="#onBtSumAction" prefHeight="25.0" prefWidth="73.0" text="Sum" />
      <Label layoutX="99.0" layoutY="210.0" prefHeight="17.0" prefWidth="52.0" text="Result = " />
      <Label fx:id="labelResult" layoutX="164.0" layoutY="210.0" prefHeight="17.0" prefWidth="106.0" />
   </children>
</AnchorPane>
